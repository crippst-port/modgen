<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="db" VERSION="20250101" COMMENT="Cache table for aiplacement_modgen">
    <TABLES>
        <TABLE NAME="aiplacement_modgen_cache" COMMENT="Cache for explore insights">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
                <FIELD NAME="courseid" TYPE="int" LENGTH="10" NOTNULL="true" COMMENT="Course ID"/>
                <FIELD NAME="data" TYPE="text" LENGTH="medium" NOTNULL="true" COMMENT="Cached insights JSON"/>
                <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" COMMENT="When cache was created"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
                <KEY NAME="courseid_fk" TYPE="foreign" FIELDS="courseid" REFTABLE="course" REFFIELDS="id"/>
            </KEYS>
        </TABLE>
    </TABLES>
</XMLDB>

